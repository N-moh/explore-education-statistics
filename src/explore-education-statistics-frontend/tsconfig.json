  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "alwaysStrict": true,
    "module": "esnext",
    "moduleResolution": "node",
    "isolatedModules": true,
    "noEmit": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "jsx": "preserve"
  }
}
